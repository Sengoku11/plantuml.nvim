{
  "$schema": "https://raw.githubusercontent.com/LuaLS/vscode-lua/master/setting/schema.json",
  "runtime": {
    "version": "LuaJIT"
  },
  "diagnostics": {
    "globals": [
      "vim"
    ]
  },
  "workspace": {
    "checkThirdParty": false,
    "library": [
      "$VIMRUNTIME",
      "$VIMRUNTIME/lua",
      "${3rd}/luv/library"
    ]
  },
  "telemetry": {
    "enable": false
  }
}
